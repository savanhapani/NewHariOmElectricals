<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>UploadServlet_cate</servlet-name>
        <servlet-class>UploadServlet_cate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet_subcate</servlet-name>
        <servlet-class>UploadServlet_subcate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UploadServlet_Product</servlet-name>
        <servlet-class>UploadServlet_Product</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UploadServlet_cate</servlet-name>
        <url-pattern>/UploadServlet_cate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet_subcate</servlet-name>
        <url-pattern>/UploadServlet_subcate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UploadServlet_Product</servlet-name>
        <url-pattern>/UploadServlet_Product</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
